{
  "name": "fastmcp-boilerplate",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "bun build ./src/server.ts --outdir ./dist --target node --minify --external sury --external effect --external @valibot/to-json-schema --external arktype --external valibot",
    "start": "bun src/server.ts",
    "dev": "bun x fastmcp dev src/server.ts",
    "inspect": "mcp-inspector bun src/server.ts",
    "lint": "biome check . && tsc --noEmit",
    "test": "bun test",
    "format": "biome check --write ."
  },
  "keywords": [
    "fastmcp",
    "mcp",
    "boilerplate"
  ],
  "repository": {
    "url": "https://github.com/punkpeye/fastmcp-boilerplate"
  },
  "author": "Frank Fiegel <frank@glama.ai>",
  "homepage": "https://glama.ai/mcp",
  "type": "module",
  "license": "MIT",
  "description": "A boilerplate for FastMCP",
  "dependencies": {
    "fastmcp": "^3.31.0",
    "zod": "^4.3.6"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@modelcontextprotocol/inspector": "^0.19.0",
    "@types/node": "^25.1.0",
    "bun-types": "^1.3.8",
    "typescript": "^5.8.3"
  }
}
